    "css:watch": "yarn css --watch"
  },
  "dependencies": {
    "@qni/elements": "^0.0.21",
    "@qni/simulator": "^0.0.21",
    "cookie-parser": "~1.4.4",
    "debug": "~4.3",
    "ejs": "~3.1",
    "express": "~4.17",
    "http-errors": "~2.0",
    "morgan": "~1.10"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "ejs-lint": "^1.2.1",
    "esbuild": "^0.14.5",
    "rollup": "^2.61.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "tailwindcss": "~3.0"
  },
  "gitHead": "a735e8013d45ba56fef6f3b6fcec597177f4c779"
