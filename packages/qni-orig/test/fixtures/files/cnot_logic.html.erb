<%= component 'circuit', nqubit: 3 do %>
  <%= circuit_register_group do %>
    <%= register_label label: 'a' %>
    <%= register_label label: 'b' %>
    <%= register_label label: 'c' %>
  <% end %>
  <%= component 'circuit/step' do %>
    <%= component 'qubit_label', label: '0x1' %>
    <%= component 'qubit_label', label: '0x1' %>
    <%= component 'qubit_label', label: '0x1' %>
  <% end %>
  <%= component 'circuit/step' do %>
    <%= wire active: false %>
    <%= wire active: false %>
    <%= component 'rw', type: :write, value: 0 %>
  <% end %>
  <%= component 'circuit/step' do %>
    <%= wire active: false %>
    <%= wire active: false %>
    <%= component 'not_gate' %>
  <% end %>
  <%= component 'circuit/step' do %>
    <%= wire active: false %>
    <%= wire active: false %>
    <%= component 'rw', type: :readout %>
  <% end %>
  <%= component 'circuit/step' do %>
    <%= wire active: false %>
    <%= component 'rw', type: :write, value: 1 %>
    <%= wire active: false %>
  <% end %>
  <%= component 'circuit/step' do %>
    <%= wire active: false %>
    <%= wire %>
    <%= component 'rw', type: :write, value: 0 %>
  <% end %>
  <%= component 'circuit/step' do %>
    <%= wire active: false %>
    <%= component 'control_dot', targets: [2], bottom: true %>
    <%= component 'not_gate', controls: [1], top: true %>
  <% end %>
  <%= component 'circuit/step' do %>
    <%= wire active: false %>
    <%= component 'rw', type: :readout %>
    <%= wire %>
  <% end %>
  <%= component 'circuit/step' do %>
    <%= wire active: false %>
    <%= wire active: false %>
    <%= component 'rw', type: :readout %>
  <% end %>
  <%= component 'circuit/step' do %>
    <%= component 'rw', type: :write, value: 1 %>
    <%= component 'rw', type: :write, value: 1 %>
    <%= component 'rw', type: :write, value: 0 %>
  <% end %>
  <%= component 'circuit/step' do %>
    <%= component 'control_dot', targets: [2], bottom: true %>
    <%= component 'control_dot', targets: [2], top: true, bottom: true %>
    <%= component 'not_gate', top: true %>
  <% end %>
  <%= component 'circuit/step' do %>
    <%= component 'rw', type: :readout %>
    <%= component 'rw', type: :readout %>
    <%= component 'rw', type: :readout %>
  <% end %>
<% end %>
