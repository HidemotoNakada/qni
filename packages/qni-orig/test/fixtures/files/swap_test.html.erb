<%= circuit nqubit: 3 do %>
  <%= circuit_block_divider do %>
    <%= circuit_register_group do %>
      <%= register_label label: 'Input 1' %>
      <%= register_label label: 'Input 2' %>
      <%= register_label label: 'Output' %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_column do %>
      <%= qubit_label label: '0x1' %>
      <%= qubit_label label: '0x1' %>
      <%= qubit_label label: '0x1' %>
    <% end %>
  <% end %>
  <%= circuit_block label: 'initialize' do %>
    <%= circuit_column do %>
      <%= write value: 0 %>
      <%= wire active: false %>
      <%= wire active: false %>
    <% end %>
    <%= circuit_column do %>
      <%= wire %>
      <%= write value: 0 %>
      <%= wire active: false %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_column do %>
      <%= wire %>
      <%= wire %>
      <%= wire active: false %>
    <% end %>
  <% end %>
  <%= circuit_block label: 'swap test' do %>
    <%= circuit_column do %>
      <%= wire %>
      <%= wire %>
      <%= write value: 0 %>
    <% end %>
    <%= circuit_column do %>
      <%= wire %>
      <%= wire %>
      <%= hadamard_gate %>
    <% end %>
    <%= circuit_column do %>
      <%= down_wire %>
      <%= up_wire bottom: true %>
      <%= control_dot top: true %>
    <% end %>
    <%= circuit_column do %>
      <%= wire %>
      <%= wire %>
      <%= hadamard_gate %>
    <% end %>
    <%= circuit_column do %>
      <%= wire %>
      <%= wire %>
      <%= not_gate %>
    <% end %>
    <%= circuit_column do %>
      <%= wire %>
      <%= wire %>
      <%= readout value: 1 %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_column do %>
      <%= wire %>
      <%= wire %>
      <%= wire active: false %>
    <% end %>
  <% end %>
<% end %>
