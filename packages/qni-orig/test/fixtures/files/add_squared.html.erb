<%= circuit nqubit: 6 do %>
  <%= circuit_block_divider do %>
    <%= circuit_register_group_span rows: 6 do %>
      <%= register_label_span label: 'a', span: 4, start: 3 %>
      <%= register_label_span label: 'b', span: 2, start: 1 %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_step do %>
      <%= qubit_label '0x1' %>
      <%= qubit_label '0x2' %>
      <%= qubit_label '0x4' %>
      <%= qubit_label '0x8' %>
      <%= qubit_label '0x1' %>
      <%= qubit_label '0x2' %>
    <% end %>
  <% end %>
  <%= circuit_block label: 'prepare' do %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= write 1 %>
      <% end %>
      <%= dropzone do %>
        <%= write 0 %>
      <% end %>
      <%= dropzone do %>
        <%= write 0 %>
      <% end %>
      <%= dropzone do %>
        <%= write 0 %>
      <% end %>
      <%= dropzone wire_active: false %>
      <%= dropzone wire_active: false %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= hadamard_gate %>
      <% end %>
      <%= dropzone %>
      <%= dropzone wire_active: false %>
      <%= dropzone wire_active: false %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= phase_gate bit: 2, phi: 'π/4' %>
      <% end %>
      <%= dropzone %>
      <%= dropzone wire_active: false %>
      <%= dropzone wire_active: false %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= write 1 %>
      <% end %>
      <%= dropzone do %>
        <%= write 0 %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= hadamard_gate %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= phase_gate bit: 5, phi: 'π/2' %>
      <% end %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
  <% end %>
  <%= circuit_block label: 'a *= b' do %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= control_gate bit: 0, targets: [3], controls: [0, 1, 2, 4] %>
      <% end %>
      <%= dropzone do %>
        <%= control_gate bit: 1, targets: [3], controls: [0, 1, 2, 4] %>
      <% end %>
      <%= dropzone do %>
        <%= control_gate bit: 2, targets: [3], controls: [0, 1, 2, 4] %>
      <% end %>
      <%= dropzone do %>
        <%= not_gate bit: 3, controls: [0, 1, 2, 4] %>
      <% end %>
      <%= dropzone do %>
        <%= control_gate bit: 4, targets: [3], controls: [0, 1, 2, 4] %>
      <% end %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= control_gate bit: 0, targets: [2], controls: [0, 1, 4] %>
      <% end %>
      <%= dropzone do %>
        <%= control_gate bit: 1, targets: [2], controls: [0, 1, 4] %>
      <% end %>
      <%= dropzone do %>
        <%= not_gate bit: 2, controls: [0, 1, 4] %>
      <% end %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone do %>
        <%= control_gate bit: 4, targets: [2], controls: [0, 1, 4] %>
      <% end %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= control_gate bit: 0, targets: [1], controls: [0, 4] %>
      <% end %>
      <%= dropzone do %>
        <%= not_gate bit: 1, controls: [0, 4] %>
      <% end %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone do %>
        <%= control_gate bit: 4, targets: [1], controls: [0, 4] %>
      <% end %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= not_gate bit: 0, controls: [4], targets: [0] %>
      <% end %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone do %>
        <%= control_gate bit: 4, targets: [0], controls: [4] %>
      <% end %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= control_gate bit: 1, targets: [3], controls: [1, 2, 5] %>
      <% end %>
      <%= dropzone do %>
        <%= control_gate bit: 2, targets: [3], controls: [1, 2, 5] %>
      <% end %>
      <%= dropzone do %>
        <%= not_gate bit: 3, controls: [1, 2, 5] %>
      <% end %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone do %>
        <%= control_gate bit: 5, targets: [3], controls: [1, 2, 5] %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= control_gate bit: 1, targets: [2], controls: [1, 5] %>
      <% end %>
      <%= dropzone do %>
        <%= not_gate bit: 2, controls: [1, 5] %>
      <% end %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone do %>
        <%= control_gate bit: 5, targets: [2], controls: [1, 5] %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= not_gate bit: 1, controls: [5], targets: [1] %>
      <% end %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone do %>
        <%= control_gate bit: 5, targets: [1], controls: [5] %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= control_gate bit: 1, targets: [3], controls: [1, 2, 4, 5] %>
      <% end %>
      <%= dropzone do %>
        <%= control_gate bit: 2, targets: [3], controls: [1, 2, 4, 5] %>
      <% end %>
      <%= dropzone do %>
        <%= not_gate bit: 3, controls: [1, 2, 4, 5] %>
      <% end %>
      <%= dropzone do %>
        <%= control_gate bit: 4, targets: [3], controls: [1, 2, 4, 5] %>
      <% end %>
      <%= dropzone do %>
        <%= control_gate bit: 5, targets: [3], controls: [1, 2, 4, 5] %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= control_gate bit: 1, targets: [2], controls: [1, 4, 5] %>
      <% end %>
      <%= dropzone do %>
        <%= not_gate bit: 2, controls: [1, 4, 5] %>
      <% end %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone do %>
        <%= control_gate bit: 4, targets: [2], controls: [1, 4, 5] %>
      <% end %>
      <%= dropzone do %>
        <%= control_gate bit: 5, targets: [2], controls: [1, 4, 5] %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= not_gate bit: 1, controls: [4, 5] %>
      <% end %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone do %>
        <%= control_gate bit: 4, targets: [1], controls: [4, 5] %>
      <% end %>
      <%= dropzone do %>
        <%= control_gate bit: 5, targets: [1], controls: [4, 5] %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= control_gate bit: 2, targets: [3], controls: [2, 5] %>
      <% end %>
      <%= dropzone do %>
        <%= not_gate bit: 3, controls: [2, 5] %>
      <% end %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone do %>
        <%= control_gate bit: 5, targets: [3], controls: [2, 5] %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= not_gate bit: 2, controls: [5], targets: [2] %>
      <% end %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone wire_top: true, wire_bottom: true %>
      <%= dropzone do %>
        <%= control_gate bit: 5, targets: [2], controls: [5] %>
      <% end %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
  <% end %>
  <%= circuit_step shadow_source: true do %>
    <%= dropzone %>
    <%= dropzone %>
    <%= dropzone %>
    <%= dropzone %>
    <%= dropzone %>
    <%= dropzone %>
  <% end %>
<% end %>
