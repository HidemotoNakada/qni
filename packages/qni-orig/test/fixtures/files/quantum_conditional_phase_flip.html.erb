<%= circuit nqubit: 5 do %>
  <%= circuit_block_divider do %>
    <%= circuit_register_group_span rows: 5 do %>
      <%= register_label_span label: 'a', span: 3, start: 3 %>
      <%= register_label_span label: 'b', span: 2, start: 1 %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_step do %>
      <%= qubit_label '0x1' %>
      <%= qubit_label '0x2' %>
      <%= qubit_label '0x4' %>
      <%= qubit_label '0x1' %>
      <%= qubit_label '0x2' %>
    <% end %>
  <% end %>
  <%= circuit_block label: 'a=0,1,2,3' do %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= draggable write: true do %>
          <%= write 0 %>
        <% end %>
      <% end %>
      <%= dropzone do %>
        <%= draggable write: true do %>
          <%= write 0 %>
        <% end %>
      <% end %>
      <%= dropzone do %>
        <%= draggable write: true do %>
          <%= write 0 %>
        <% end %>
      <% end %>
      <%= dropzone wire_active: false %>
      <%= dropzone wire_active: false %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= hadamard_gate %>
        <% end %>
      <% end %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= hadamard_gate %>
        <% end %>
      <% end %>
      <%= dropzone %>
      <%= dropzone wire_active: false %>
      <%= dropzone wire_active: false %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone wire_active: false %>
      <%= dropzone wire_active: false %>
    <% end %>
  <% end %>
  <%= circuit_block label: 'b=1,3' do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= draggable write: true do %>
          <%= write 1 %>
        <% end %>
      <% end %>
      <%= dropzone do %>
        <%= draggable write: true do %>
          <%= write 0 %>
        <% end %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= hadamard_gate %>
        <% end %>
      <% end %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
  <% end %>
  <%= circuit_block label: 'a-=3' do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= not_gate bit: 1 %>
        <% end %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= control_gate bit: 1, targets: [2], controls: [1] %>
        <% end %>
      <% end %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= not_gate bit: 2, controls: [1], targets: [2] %>
        <% end %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= not_gate bit: 0 %>
        <% end %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= control_gate bit: 0, targets: [1], controls: [0] %>
        <% end %>
      <% end %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= not_gate bit: 1, controls: [0], targets: [1] %>
        <% end %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= control_gate bit: 0, targets: [2], controls: [0, 1] %>
        <% end %>
      <% end %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= control_gate bit: 1, targets: [2], controls: [0, 1] %>
        <% end %>
      <% end %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= not_gate bit: 2, controls: [0, 1] %>
        <% end %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
  <% end %>
  <%= circuit_block label: 'flip if (a<0 && b==1)' do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= not_gate bit: 4 %>
        <% end %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= control_gate bit: 2, targets: [2, 3, 4], controls: [2, 3, 4] %>
        <% end %>
      <% end %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= control_gate bit: 3, targets: [2, 3, 4], controls: [2, 3, 4] %>
        <% end %>
      <% end %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= control_gate bit: 4, targets: [2, 3, 4], controls: [2, 3, 4] %>
        <% end %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= not_gate bit: 4 %>
        <% end %>
      <% end %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
  <% end %>
  <%= circuit_block label: 'a+=3' do %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= control_gate bit: 0, targets: [2], controls: [0, 1] %>
        <% end %>
      <% end %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= control_gate bit: 1, targets: [2], controls: [0, 1] %>
        <% end %>
      <% end %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= not_gate bit: 2, controls: [0, 1] %>
        <% end %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= control_gate bit: 0, targets: [1], controls: [0] %>
        <% end %>
      <% end %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= not_gate bit: 1, controls: [0], targets: [1] %>
        <% end %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= not_gate bit: 0 %>
        <% end %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= control_gate bit: 1, targets: [2], controls: [1] %>
        <% end %>
      <% end %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= not_gate bit: 2, controls: [1], targets: [2] %>
        <% end %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= draggable do %>
          <%= not_gate bit: 1 %>
        <% end %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
  <% end %>
  <%= circuit_step shadow_source: true do %>
    <%= dropzone %>
    <%= dropzone %>
    <%= dropzone %>
    <%= dropzone %>
    <%= dropzone %>
  <% end %>
<% end %>
