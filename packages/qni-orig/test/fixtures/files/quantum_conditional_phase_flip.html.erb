<%= circuit nqubit: 5 do %>
  <%= circuit_block_divider do %>
    <%= circuit_step do %>
      <%= qubit_label '0x1' %>
      <%= qubit_label '0x2' %>
      <%= qubit_label '0x4' %>
      <%= qubit_label '0x8' %>
      <%= qubit_label '0x10' %>
    <% end %>
  <% end %>
  <%= circuit_block label: 'a=0,1,2,3' do %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= write 0 %>
      <% end %>
      <%= dropzone do %>
        <%= write 0 %>
      <% end %>
      <%= dropzone do %>
        <%= write 0 %>
      <% end %>
      <%= dropzone wire_active: false %>
      <%= dropzone wire_active: false %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= hadamard_gate bit: 0 %>
      <% end %>
      <%= dropzone do %>
        <%= hadamard_gate bit: 1 %>
      <% end %>
      <%= dropzone %>
      <%= dropzone wire_active: false %>
      <%= dropzone wire_active: false %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone wire_active: false %>
      <%= dropzone wire_active: false %>
    <% end %>
  <% end %>
  <%= circuit_block label: 'b=1,3' do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= write 1 %>
      <% end %>
      <%= dropzone do %>
        <%= write 0 %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= hadamard_gate bit: 4 %>
      <% end %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
  <% end %>
  <%= circuit_block label: 'a-=3' do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= not_gate bit: 1 %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= control_gate bit: 1, targets: [2], controls: [1] %>
      <% end %>
      <%= dropzone do %>
        <%= not_gate bit: 2, controls: [1] %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= not_gate bit: 0 %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= control_gate bit: 0, targets: [1], controls: [0] %>
      <% end %>
      <%= dropzone do %>
        <%= not_gate bit: 1, controls: [0] %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= control_gate bit: 0, targets: [2], controls: [0, 1] %>
      <% end %>
      <%= dropzone do %>
        <%= control_gate bit: 1, targets: [2], controls: [0, 1] %>
      <% end %>
      <%= dropzone do %>
        <%= not_gate bit: 2, controls: [0, 1] %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
  <% end %>
  <%= circuit_block label: 'flip if (a<0 && b==1)' do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= not_gate bit: 4 %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= control_gate bit: 2, targets: [2, 3, 4], controls: [2, 3, 4] %>
      <% end %>
      <%= dropzone do %>
        <%= control_gate bit: 3, targets: [2, 3, 4], controls: [2, 3, 4] %>
      <% end %>
      <%= dropzone do %>
        <%= control_gate bit: 4, targets: [2, 3, 4], controls: [2, 3, 4] %>
      <% end %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= not_gate bit: 4 %>
      <% end %>
    <% end %>
  <% end %>
  <%= circuit_block_divider do %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
  <% end %>
  <%= circuit_block label: 'a+=3' do %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= control_gate bit: 0, targets: [2], controls: [0, 1] %>
      <% end %>
      <%= dropzone do %>
        <%= control_gate bit: 1, targets: [2], controls: [0, 1] %>
      <% end %>
      <%= dropzone do %>
        <%= not_gate bit: 2, controls: [0, 1] %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= control_gate bit: 0, targets: [1], controls: [0] %>
      <% end %>
      <%= dropzone do %>
        <%= not_gate bit: 1, controls: [0] %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone do %>
        <%= not_gate bit: 0 %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= control_gate bit: 1, targets: [2], controls: [1] %>
      <% end %>
      <%= dropzone do %>
        <%= not_gate bit: 2, controls: [1] %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
    <%= circuit_step do %>
      <%= dropzone %>
      <%= dropzone do %>
        <%= not_gate bit: 1 %>
      <% end %>
      <%= dropzone %>
      <%= dropzone %>
      <%= dropzone %>
    <% end %>
  <% end %>
  <%= circuit_step shadow_source: true do %>
    <%= dropzone %>
    <%= dropzone %>
    <%= dropzone %>
    <%= dropzone %>
    <%= dropzone %>
  <% end %>
<% end %>
