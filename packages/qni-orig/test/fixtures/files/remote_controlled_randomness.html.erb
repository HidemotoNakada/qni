<%= circuit nqubit: 2 do %>
  <%= circuit_register_group do %>
    <%= register_label 'a' %>
    <%= register_label 'b' %>
  <% end %>
  <%= circuit_step do %>
    <%= qubit_label '0x1' %>
    <%= qubit_label '0x1' %>
  <% end %>
  <%= circuit_step do %>
    <%= dropzone do %>
      <%= write 0 %>
    <% end %>
    <%= dropzone do %>
      <%= write 0 %>
    <% end %>
  <% end %>
  <%= circuit_step do %>
    <%= dropzone do %>
      <%= hadamard_gate %>
    <% end %>
    <%= dropzone %>
  <% end %>
  <%= circuit_step do %>
    <%= dropzone %>
    <%= dropzone do %>
      <%= hadamard_gate %>
    <% end %>
  <% end %>
  <%= circuit_step do %>
    <%= dropzone %>
    <%= dropzone do %>
      <%= phase_gate bit: 1, phi: 'Ï€/4' %>
    <% end %>
  <% end %>
  <%= circuit_step do %>
    <%= dropzone %>
    <%= dropzone do %>
      <%= hadamard_gate %>
    <% end %>
  <% end %>
  <%= circuit_step do %>
    <%= dropzone do %>
      <%= control_gate bit: 0, targets: [1], controls: [0] %>
    <% end %>
    <%= dropzone do %>
      <%= not_gate bit: 1, controls: [0], targets: [1] %>
    <% end %>
  <% end %>
  <%= circuit_step do %>
    <%= dropzone do %>
      <%= readout %>
    <% end %>
    <%= dropzone %>
  <% end %>
  <%= circuit_step do %>
    <%= dropzone wire_active: false %>
    <%= dropzone do %>
      <%= readout %>
    <% end %>
  <% end %>
  <%= circuit_step shadow_source: true do %>
    <%= dropzone %>
    <%= dropzone %>
  <% end %>
<% end %>
