<% circuit = local_assigns.fetch(:circuit) %>

<div id="circuit"
     class="circuit"
     data-simulator-target="circuit"
     data-nqubit="<%= circuit.nqubit %>">
  <div class="circuit__body"
       data-action="mouseleave->simulator#maybeBackToCurrentBreakpoint:passive">
    <%= circuit.content %>
  </div>
</div>
