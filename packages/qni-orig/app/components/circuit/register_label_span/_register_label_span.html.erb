<% register_label_span = local_assigns.fetch(:register_label_span) %>
<% label = register_label_span.label %>
<% start = register_label_span.start %>
<% span = register_label_span.span %>

<div class="register-label-span reg-start-<%= start %> reg-end-<%= start + span %> span-<%= span %>">
  <span class="register-label-span__label"><%= label %></span>
</div>
