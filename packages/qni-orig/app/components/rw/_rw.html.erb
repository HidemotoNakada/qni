<% rw = local_assigns.fetch(:rw) %>

<%= tag.div class: class_string('circuit-element', 'rw', 'gate', rw.type, "rw--#{rw.value}" => rw.value), data: { value: rw.value, set: rw.set } do %>
  <div class="rw__wire rw__wire--left"></div>
  <div class="rw__wire rw__wire--right"></div>
  <div class="rw__body">
    <div>
      |<span class="rw__value"></span>&#10217;
    </div>
  </div>
  <%= circuit_element_label label: rw.set %>
<% end %>
