<% kets = (0..2**Qni::MAX_NQUBIT - 1).to_a %>

<%= state_vector do %>
  <%= qubit_circle_group(kets, size: 64) do |qc64| %>
    <%= qubit_circle_group(qc64) do |qc32| %>
      <%= qubit_circle_group(qc32) do |qc16| %>
        <%= qubit_circle_group qc16 %>
      <% end %>
    <% end %>
  <% end %>
<% end %>
