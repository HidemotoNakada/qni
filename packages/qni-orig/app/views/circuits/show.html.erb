<% content_for :head do %>
  <%= javascript_pack_tag 'service_worker', 'data-turbolinks-track': 'reload' %>
<% end %>

<%= component 'navbar', title: @circuit.name %>

<%= container do %>
  <%= simulator_wrapper nqubit: @circuit.nqubit do %>
    <%= component 'simulator', nqubit: @circuit.nqubit do %>
      <%= circuit_view circuit: @circuit %>
      <%= circle_notation @circuit %>
      <%= component 'run_button' %>
    <% end %>
  <% end %>
<% end %>
